@model IEnumerable<Comment>
<table class="table">
  <tr>
    <th>
      @Html.DisplayNameFor(model => model.SessionId)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.SessionContentNote)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.SpeakerNote)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.SupportNote)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.CommentText)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.ImageUrl)
    </th>
    <th></th>
  </tr>

  @foreach (var item in Model)
  {
    <tr>
      <td>
        @Html.DisplayFor(modelItem => item.SessionId)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.SessionContentNote)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.SpeakerNote)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.SupportNote)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.CommentText)
      </td>
        <td>
            <img src="@item.ImageUrl"/>
        </td>
    </tr>
  }

</table>

